<?xml version="1.0" encoding="UTF-8"?>

<struts-config>
	<!-- ========== Data Source Configuration === -->
	<!-- ========== Form Bean Definitions === -->
	<form-beans>
		<form-bean name="UpdateScheduleForm"
			type="sg.com.customer.JAVA01.model.UpdateScheduleForm" />
	</form-beans>
	<!--========== Global Forward Definitions === -->
	<global-forwards>
	</global-forwards>
	<!-- ========== Action Mapping Definitions === -->
	<action-mappings>
		<action path="/infoSchedule"
			type="sg.com.customer.JAVA01.webapp.action.GetInfoScheduleAction"
			name="UpdateScheduleForm" scope="request" input="ListSchedule.jsp">
			<forward name="success" path="/UpdateSchedule.jsp"></forward>
			<forward name="failure" path="/Failure.jsp"></forward>
		</action>
	</action-mappings>

	<action-mappings>
		<action path="/updateSchedule"
			type="sg.com.customer.JAVA01.webapp.action.UpdateScheduleAction"
			name="UpdateScheduleForm" scope="request" input="UpdateSchedule.jsp">
			<forward name="success" path="/UpdateSuccess.jsp"></forward>
			<forward name="failure" path="/Failure.jsp"></forward>
		</action>
	</action-mappings>
</struts-config>